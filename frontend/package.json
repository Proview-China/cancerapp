{
  "name": "cancerapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "kill-port": "node ./scripts/kill-port.cjs 5173",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:start": "wait-on tcp:localhost:5173 && cross-env NODE_ENV=development electron electron/main.cjs --no-sandbox --disable-gpu-sandbox",
    "electron:dev": "npm run kill-port && concurrently \"npm run dev\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "mammoth": "^1.7.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^9.0.3",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^33.2.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "wait-on": "^7.2.0"
  }
}
